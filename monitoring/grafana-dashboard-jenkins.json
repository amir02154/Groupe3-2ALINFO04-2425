{
  "id": null,
  "uid": "jenkins-creative-dashboard",
  "title": "Jenkins Creative Dashboard",
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "Build Success Rate (%)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(jenkins_job_builds_successful_total) / (sum(jenkins_job_builds_successful_total) + sum(jenkins_job_builds_failed_total)) * 100",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2
        },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Builds Failed (Total)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(jenkins_job_builds_failed_total)",
          "legendFormat": "Failed Builds",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "none" },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Builds Success (Total)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(jenkins_job_builds_successful_total)",
          "legendFormat": "Success Builds",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "none" },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Build Duration (s)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "avg(jenkins_job_last_build_duration_seconds)",
          "legendFormat": "Avg Duration",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s", "decimals": 2 },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 }
    },
    {
      "type": "stat",
      "title": "Builds in Queue",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(jenkins_queue_size)",
          "legendFormat": "Queue Size",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "none" },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 }
    },
    {
      "type": "stat",
      "title": "Jenkins CPU Usage (seconds)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(process_cpu_seconds_total{job=\"jenkins\"})",
          "legendFormat": "CPU Usage (s)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s", "decimals": 2 },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 22 }
    },
    {
      "type": "stat",
      "title": "Jenkins Memory Usage (bytes)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(process_resident_memory_bytes{job=\"jenkins\"})",
          "legendFormat": "Memory Usage (bytes)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bytes", "decimals": 2 },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 22 }
    },
    {
      "type": "table",
      "title": "Last Build Status by Job",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "jenkins_job_last_build_result",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 14 }
    },
    {
      "type": "stat",
      "title": "Jenkins JVM Heap Memory Used",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "jvm_memory_bytes_used{area=\"heap\"}",
          "legendFormat": "Heap Used",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bytes", "decimals": 2 },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 28 }
    },
    {
      "type": "stat",
      "title": "Jenkins JVM Heap Memory Max",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "jvm_memory_bytes_max{area=\"heap\"}",
          "legendFormat": "Heap Max",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bytes", "decimals": 2 },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 28 }
    },
    {
      "type": "stat",
      "title": "Jenkins JVM Total Memory Used",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(jvm_memory_bytes_used)",
          "legendFormat": "Total Used",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bytes", "decimals": 2 },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 28 }
    },
    {
      "type": "stat",
      "title": "Jenkins CPU Usage (%)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total[1m]) / 4 * 100",
          "legendFormat": "CPU %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent", "decimals": 2 },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 34 }
    }
  ],
  "templating": { "list": [] },
  "annotations": { "list": [] }
} 